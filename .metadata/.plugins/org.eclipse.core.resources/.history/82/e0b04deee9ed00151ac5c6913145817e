
package ch.hearc.p2.tools;



public class UseTools
	{
	/*------------------------------------------------------------------*\
	|*							Methodes Public							*|
	\*------------------------------------------------------------------*/

	public static void main(String[] args)
		{
		main();
		}

	public static void main()
		{
		System.out.println(Tools.getAngle(20, 20));
		System.out.println(Tools.getAngle(-20, 20));
		System.out.println(Tools.getAngle(20, -20));
		System.out.println(Tools.getAngle(-20, -20));
		System.out.println(Tools.getAngle(0, 20));
		System.out.println(Tools.getAngle(20, 0));
		System.out.println(Math.PI/2);
		}

	/*------------------------------------------------------------------*\
	|*							Methodes Private						*|
	\*------------------------------------------------------------------*/

	}

